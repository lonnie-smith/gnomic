{
    "name": "gnomic-server",
    "private": true,
    "description": "A personal website",
    "version": "0.1.0",
    "author": "Lonnie Smith <lawrence.smith.3@gmail.com>",
    "license": "MIT",
    "scripts": {
        "knex": "knex",
        "gulp": "gulp",
        "import": "node db/import/index.js",
        "dev": "NODE_ENV=development gulp & NODE_ENV=development nodemon ./server/server.js",
        "start": "gulp; node ./server/server.js"
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "body-parser": "1.18.3",
        "cross-env": "5.2.0",
        "env-merger": "1.0.0",
        "express": "4.16.4",
        "glob": "7.1.3",
        "knex": "0.16.3",
        "lodash": "4.17.11",
        "pug": "2.0.3",
        "remark": "10.0.1",
        "remark-extract-frontmatter": "2.0.0",
        "remark-frontmatter": "1.3.1",
        "remark-html": "9.0.0",
        "remark-parse": "6.0.3",
        "remark-stringify": "6.0.4",
        "sqlite3": "4.0.6",
        "to-vfile": "5.0.2",
        "unified": "7.1.0",
        "vfile-reporter": "5.1.1",
        "vue": "2.5.22",
        "vue-router": "3.0.2",
        "vuex": "3.1.0",
        "yaml": "1.2.1"
    },
    "devDependencies": {
        "@types/lodash": "4.14.120",
        "ansi-colors": "3.1.0",
        "babel-core": "6.26.3",
        "babel-plugin-transform-async-to-generator": "6.24.1",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-object-rest-spread": "6.26.0",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-env": "1.7.0",
        "babel-preset-es2015": "6.24.1",
        "babel-register": "6.26.0",
        "babelify": "8.0.0",
        "beeper": "1.1.1",
        "browser-sync": "2.26.3",
        "browserify": "16.2.3",
        "browserify-shim": "3.8.14",
        "del": "3.0.0",
        "envify": "4.1.0",
        "fancy-log": "1.3.2",
        "gulp": "4.0.0",
        "gulp-clean-css": "3.10.0",
        "gulp-if": "2.0.2",
        "gulp-plumber": "1.2.0",
        "gulp-prettify": "0.5.0",
        "gulp-rename": "1.4.0",
        "gulp-sass": "4.0.2",
        "gulp-sourcemaps": "2.6.4",
        "gulp-uglify": "3.0.1",
        "nodemon": "1.18.9",
        "vinyl-buffer": "1.0.1",
        "vinyl-source-stream": "2.0.0",
        "vueify": "9.4.1",
        "watchify": "3.11.0"
    },
    "optionalDependencies": {
        "ajv": "5.5.2",
        "babel-eslint": "10.0.1",
        "eslint": "4.19.1",
        "eslint-plugin-vue": "4.7.1",
        "gulp-eslint": "5.0.0"
    },
    "browserify-shim": {},
    "engines": {
        "node": ">=10.15.0"
    },
    "browser": {
        "vue": "vue/dist/vue.common.js"
    },
    "babel": {
        "presets": [
            [
                "env"
            ]
        ],
        "plugins": [
            "transform-class-properties",
            "transform-object-rest-spread",
            "transform-async-to-generator",
            "transform-runtime"
        ]
    },
    "nodemonConfig": {
        "restartable": "rs",
        "ignore": [
            "server/static"
        ],
        "watch": [
            "server/",
            "db/"
        ],
        "ext": "js,json,pug"
    }
}
